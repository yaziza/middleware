package de.tud.in.middleware.costumers;

import javax.ejb.LocalBean;
import javax.ejb.Stateless;
import javax.jws.WebService;

/**
 * Session Bean implementation class CostumerManagement
 */
@WebService
@Stateless
@LocalBean
public class CostumerManagement implements CostumerManagementRemote, CostumerManagementLocal {

    /**
     * Default constructor. 
     */
    public CostumerManagement() {
        // TODO Auto-generated constructor stub
    }

	@Override
	public long getNumberOfCustomers() {
		// TODO Auto-generated method stub
		return 1;
	}

}
